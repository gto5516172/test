<?xml version="1.0" encoding="UTF-8"?>
<excel>
	<!-- 
		必填项为:name,header，其他全部选填
		
		title:excel表格中第一行合并单元格居中显示的内容
		description：第二行显示的说明
		author：xml作者，不会输出
		class:类路径
		sheet:导出excel的sheet名
		columns:导出列的信息
			column:一个列
				name:列对应Object中的字段名，必须有标准的get方法
				header:导出excel中显示的标题
				width:列宽(单位像素)
				type:列的类型，读取时需要用到
	 -->
	<!-- 读取配置 开始 -->
	<class>com.bety.web.model.CommoditySpu</class>
	<sheetNum>0</sheetNum>
	<startRow>1</startRow>
	<!-- 读取配置 结束 -->
	<author>BetyChen</author>
	<sheet>Excel导入</sheet>
	<columns>
	<column>
			<name>name</name>
			<type>java.lang.String</type>
			<header>商品名称</header>
			<width>50</width>
		</column>
		<column>
			<name>spuNo</name>
			<type>java.lang.String</type>
			<header>商品SPU码</header>
			<width>50</width>
		</column>
		<column>
			<name>typeTmp</name>
			<type>java.lang.String</type>
			<header>分类</header>
			<width>200</width>
		</column>
		<column>
			<name>price</name>
			<type>java.lang.String</type>
			<header>建议零售价</header>
			<width>200</width>
		</column>
		<column>
			<name>brand</name>
			<type>java.lang.String</type>
			<header>品牌</header>
			<width>200</width>
		</column>
		<column>
			<name>manufactorNo</name>
			<type>java.lang.String</type>
			<header>原厂编号</header>
			<width>200</width>
		</column>
		<column>
			<name>units</name>
			<type>java.lang.String</type>
			<header>单位</header>
			<width>200</width>
		</column>
		<column>
			<name>originPlace</name>
			<type>java.lang.String</type>
			<header>商品产地</header>
			<width>200</width>
		</column>
		<column>
			<name>descr</name>
			<type>java.lang.String</type>
			<header>描述</header>
			<width>200</width>
		</column>
		<column>
			<name>sellDescr</name>
			<type>java.lang.String</type>
			<header>销售信息</header>
			<width>200</width>
		</column>
		<column>
			<name>otherDescr</name>
			<type>java.lang.String</type>
			<header>其他备注</header>
			<width>200</width>
		</column>
		<column>
			<name>title</name>
			<type>java.lang.String</type>
			<header>产品标题 </header>
			<width>200</width>
		</column>
		<column>
			<name>plfTmp</name>
			<type>java.lang.String</type>
			<header>平台</header>
			<width>200</width>
		</column>
		<column>
			<name>keywords</name>
			<type>java.lang.String</type>
			<header>关键词</header>
			<width>200</width>
		</column>
		<column>
			<name>descs</name>
			<type>java.lang.String</type>
			<header>产品描述</header>
			<width>200</width>
		</column>
	</columns>
</excel>