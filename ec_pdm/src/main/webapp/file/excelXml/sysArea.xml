<?xml version="1.0" encoding="UTF-8"?>
<excel>
	<!-- 
		必填项为:name,header，其他全部选填
		
		title:excel表格中第一行合并单元格居中显示的内容
		description：第二行显示的说明
		author：xml作者，不会输出
		class:类路径
		sheet:导出excel的sheet名
		columns:导出列的信息
			column:一个列
				name:列对应Object中的字段名，必须有标准的get方法
				header:导出excel中显示的标题
				width:列宽(单位像素)
				type:列的类型，读取时需要用到
	 -->
	<!-- 读取配置 开始 -->
	<class>com.gohuinuo.web.sys.model.SysArea</class>
	<sheetNum>0</sheetNum>
	<startRow>1</startRow>
	<!-- 读取配置 结束 -->
	<author>guoyf</author>
	<sheet>区域</sheet>
	<columns>
		<column>
			<name>name</name>
			<type>java.lang.String</type>
			<header>区域名称</header>
			<width>50</width>
		</column>
		<column>
			<name>code</name>
			<type>java.lang.String</type>
			<header>区域编码</header>
			<width>200</width>
		</column>
	</columns>
</excel>