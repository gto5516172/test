<?xml version="1.0" encoding="UTF-8"?>
<excel>
	<!-- 
		必填项为:name,header，其他全部选填
		
		title:excel表格中第一行合并单元格居中显示的内容
		description：第二行显示的说明
		author：xml作者，不会输出
		class:类路径
		sheet:导出excel的sheet名
		columns:导出列的信息
			column:一个列
				name:列对应Object中的字段名，必须有标准的get方法
				header:导出excel中显示的标题
				width:列宽(单位像素)
				type:列的类型，读取时需要用到
	 -->
	<!-- 读取配置 开始 -->
	<class>com.gyf.ec.model.EcSuppliers</class>
	<sheetNum>0</sheetNum>
	<startRow>1</startRow>
	<!-- 读取配置 结束 -->
	<author>guoyf</author>
	<sheet>供应商</sheet>
	<columns>
		<column>
			<name>suppliersName</name>
			<type>java.lang.String</type>
			<header>供应商名称</header>
			<width>50</width>
		</column>
		<column>
			<name>abbreviation</name>
			<type>java.lang.String</type>
			<header>助记简称</header>
			<width>200</width>
		</column>
		<column>
			<name>suppliersType</name>
			<type>java.lang.Integer</type>
			<header>供应商类别</header>
		</column>
		<column>
			<name>suppliersWeb</name>
			<type>java.lang.String</type>
			<header>官方网址</header>
			<width>200</width>
		</column>
		<column>
			<name>taxIdentificationNumber</name>
			<type>java.lang.String</type>
			<header>纳税识别号</header>
			<width>200</width>
		</column>
		<column>
			<name>taxTime</name>
			<type>java.lang.String</type>
			<header>一般纳税认定时间</header>
			<width>200</width>
		</column>
		<column>
			<name>personName1</name>
			<type>java.lang.String</type>
			<header>联系人1</header>
			<width>200</width>
		</column>
		<column>
			<name>personPhone1</name>
			<type>java.lang.String</type>
			<header>联系电话1</header>
			<width>200</width>
		</column>
		<column>
			<name>suppliersAddr</name>
			<type>java.lang.String</type>
			<header>供应商地址</header>
			<width>1000</width>
		</column>
		<column>
			<name>qq</name>
			<type>java.lang.String</type>
			<header>QQ</header>
			<width>200</width>
		</column>
		<column>
			<name>email</name>
			<type>java.lang.String</type>
			<header>E-mail</header>
			<width>200</width>
		</column>
		<column>
			<name>remarks</name>
			<type>java.lang.String</type>
			<header>备注</header>
			<width>200</width>
		</column>
	</columns>
</excel>